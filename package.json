{
  "name": "api",
  "version": "1.0.0",
  "description": "Node.js API for the single-cell pipeline.",
  "main": "index.js",  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src",
    "prod": "tsc && node ./built/src/app.js",
    "debug": "tsc && node --inspect ./built/src/app.js",
    "test": "jest --silent --testTimeout=10000",
    "test:loud": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --silent",
    "dev": "ts-node-dev --respawn --transpile-only ./src/app.ts",
    "coverage": "jest --coverage --silent",
    "start": "npm run dev",
    "detect-secrets": "pip3 install detect-secrets && git diff --staged --name-only | xargs detect-secrets-hook --baseline .secrets.baseline"
  },
  "pre-commit": [
    "lint",
    "detect-secrets"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/biomage/api.git"
  },
  "author": "Biomage, Ltd.",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/biomage/api/issues"
  },
  "homepage": "https://gitlab.com/biomage/api#readme",
  "dependencies": {
    "@kubernetes/client-node": "^0.12.3",
    "async-lock": "^1.3.0",
    "aws-sdk": "^2.847.0",
    "aws-xray-sdk": "^3.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-openapi-validator": "^4.10.8",
    "github-download-directory": "^2.0.0",
    "ioredis": "^4.17.3",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "lodash": "^4.17.21",
    "log-timestamp": "^0.3.0",
    "lru-cache": "^5.1.1",
    "micromatch": "^4.0.2",
    "node-cleanup": "^2.1.2",
    "node-fetch": "^2.6.1",
    "node-jq": "^2.0.0",
    "object-hash": "^2.0.3",
    "sns-validator": "^0.3.4",
    "socket.io": "^3.1.2",
    "socket.io-redis": "^6.0.1",
    "swagger-client": "^3.12.2",
    "swagger-model-validator": "^3.0.18",
    "uuid": "^8.3.2",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.3",
    "@types/aws-sdk": "^2.7.0",
    "@types/babel-core": "^6.25.7",
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^7.28.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/ioredis": "^4.27.1",
    "@types/ioredis-mock": "^5.6.0",
    "@types/jest": "^27.0.1",
    "@types/js-yaml": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/lodash": "^4.14.172",
    "@types/lru-cache": "^5.1.1",
    "@types/micromatch": "^4.0.2",
    "@types/node-cleanup": "^2.1.2",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemon": "^1.19.1",
    "@types/object-hash": "^2.1.1",
    "@types/sns-validator": "^0.3.0",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/socket.io-redis": "^3.0.0",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@types/yaml": "^1.9.7",
    "aws-sdk-mock": "^5.1.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "ioredis-mock": "^4.21.1",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "nodemon": "^2.0.3",
    "pre-commit": "^1.2.2",
    "socket.io-client": "^2.3.0",
    "socket.io-mock": "^1.3.1",
    "supertest": "^4.0.2",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/src/.*/__mocks__"
    ]
  }
